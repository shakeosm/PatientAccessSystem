<!-- HeaderLayout.cshtml -->
<meta charset="utf-8">
<title>@ViewData["Title"] - PAS</title>
<meta name="description" content="WebApp Home Page">
<meta http-equiv="X-UA-Compatible" content="IE=edge">
<meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no, user-scalable=no, minimal-ui">
<!-- Call App Mode on ios devices -->
<meta name="apple-mobile-web-app-capable" content="yes" />
<!-- Remove Tap Highlight on Windows Phone IE -->
<meta name="msapplication-tap-highlight" content="no">
<!-- base css -->
<!-- For Optimization- try to use Bootstrap Styles later- and avoid the "Vendor- js/css" -->
<link id="vendorsbundle" rel="stylesheet" media="screen, print" href="~/css/vendors.bundle.css"> @*723 kb*@
<link id="appbundle" rel="stylesheet" media="screen, print" href="~/css/app.bundle.css"> @*240KB*@

<link id="myskin" rel="stylesheet" media="screen, print" href="~/css/skins/skin-master.css">

@*The fa prefix has been deprecated in Font-awesome-version 5. The new default is the fas solid style and the fab style for brands.*@
<link rel="stylesheet" media="screen, print" href="~/css/fa-solid.css">
<link rel="stylesheet" media="screen, print" href="~/css/fa-brands.css">

<!-- base css -->
<link href="~/css/app-custom-style.css" rel="stylesheet" />

@*<link id="vendorsbundle" rel="stylesheet" media="screen, print" href="~/css/vendors.bundle.css">*@ @* included in '_HeaderLayout' *@
